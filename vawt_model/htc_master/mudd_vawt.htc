begin simulation ;
  time_stop    150;
  solvertype   1 ;    (newmark)
  on_no_convergence continue ;
  convergence_limits 0.1 1 1e-7 ;
  logfile   ./log/H-rotor_3blades.log;
  visualization ./visualization/H-rotor_3blades.hdf5;
  begin newmark;
    beta      0.27;
    gamma     0.51;  
    deltat    0.001;
    bdynamic  1.0 ;
  end newmark;
end simulation;
begin new_htc_structure;
   begin main_body;  tower 3.0m  TODO! Fix me.
    name        tower ;            
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
    damping_posdef 0 0 0 1e-8 1e-8 1e-8 ;  add damping for numerical stability
	gravity 0;
	begin timoschenko_input;
      filename ./data/Stiff.st ;
      set 1 1 ; 
    end timoschenko_input;
    begin c2_def;              Definition of centerline (main_body coordinates)
      nsec 2;
      sec 1 0.0 0.0  0.0   0.0 ;  x,y,z,twist
      sec 2 0.0 0.0 -3.0  0.0 ;
      end c2_def ;
    end main_body;
  begin main_body;  tower 2.0m   TODO! Fix me. 
    name        towertop ;
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
    damping_posdef 0 0 0 1e-8 1e-8 1e-8 ;  add damping for numerical stability
	gravity 0;
	begin timoschenko_input;
      filename ./data/Stiff.st ;
      set 1 1 ;                
    end timoschenko_input;
    begin c2_def;              Definition of centerline (main_body coordinates)
      nsec 2;
      sec 1 0.0 0.0  0.0  0.0 ; x,y,z,twist
      sec 2 0.0 0.0 -2.0  0.0 ; 
    end c2_def ;
  end main_body;
  begin main_body;  hub1  0.1m
    name        hub1 ;
    type        timoschenko ;
    nbodies     1 ;
    node_distribution     c2_def ;
    gravity 0;    	
	begin timoschenko_input;
      filename ./data/Stiff.st ;
      set 1 1;                
    end timoschenko_input;
    begin c2_def;              Definition of centerline (main_body coordinates)
      nsec 2;
      sec 1 0.0 0.0 0.0     0.0 ; x,y,z,twist
      sec 2 0.0 0.0 0.1     0.0 ; 
    end c2_def ;
  end main_body;
  begin main_body;
    name           hub2 ;
    copy_main_body hub1;
  end main_body;
  begin main_body;
    name           hub3 ;
    copy_main_body hub1;
  end main_body;
  begin main_body;  strut1  0.6874m
    name        strut1 ;            
    type        timoschenko ;
    nbodies     1 ;
    node_distribution    c2_def;
    damping_posdef 0 0 0 1e-8 1e-8 1e-8 ;  add damping for numerical stability
	gravity 0;
   begin timoschenko_input ;
      filename ./data/Stiff.st ;
      set 1 1 ;                set subset
    end timoschenko_input;
    begin c2_def;              Definition of centerline (main_body coordinates)
      nsec 2 ;
		sec	1	0.0		0.0		0.0		0.0	;
		sec	2	0.0		0.0		0.6874		0.0	;
	end c2_def ;
  end main_body;
  begin main_body;
    name           strut2 ;
    copy_main_body strut1;
  end main_body;
  begin main_body;
    name           strut3 ;
    copy_main_body strut1;
  end main_body;
  begin main_body;  blade1  1.8288m
    name        blade1 ;        
    type        timoschenko ;
    nbodies     1 ;
    node_distribution    c2_def;
	gravity 0;
   begin timoschenko_input ;
      filename ./data/Stiff.st ;
      set 1 1 ;                set subset
    end timoschenko_input;
    begin c2_def;              Definition of centerline (main_body coordinates)
      nsec 3 ;
		sec	1	0.0		0.0		0		0.0	;
		sec	2	0.0		0.0		0.9144	0.0	;
		sec	3	0.0		0.0		1.8288	0.0	;
   end c2_def ;
   end main_body;
  begin main_body;
    name           blade2 ;
    copy_main_body blade1;
  end main_body;
  begin main_body;
    name           blade3 ;
    copy_main_body blade1;
  end main_body;
  begin orientation;
	begin base;
		mbdy   tower;
		inipos        0.0 0.0 0.0 ; initial position of node 1
		mbdy_eulerang 0.0 0.0 0.0;          
	end base;
    begin relative;  towertop to tower (initial velocity)
      mbdy1  tower last;
      mbdy2  towertop 1;
      mbdy2_eulerang 0.0 0.0 0.0; 
      mbdy2_ini_rotvec_d1 0.0 0.0 1.0 18.850 ; mbdy initial rotation velocity x.y.z.angle velocity[rad/s]  (mbdy 2 coordinates)
    end relative;
    begin relative;
      mbdy1  towertop last;
      mbdy2  hub1 1;
      mbdy2_eulerang 0.0 0.0 0.0; 	  
      mbdy2_eulerang 90.0 0.0 0.0; 
    end relative;
    begin relative;
      mbdy1  towertop last;
      mbdy2  hub2 1;
      mbdy2_eulerang 0.0 0.0 0.0; 
      mbdy2_eulerang 90.0 0.0 0.0;
	  mbdy2_eulerang 0.0 120.0 0.0; 	  
    end relative;
    begin relative;
      mbdy1  towertop last;
      mbdy2  hub3 1;
      mbdy2_eulerang 0.0 0.0 0.0; 
      mbdy2_eulerang 90.0 0.0 0.0;
	  mbdy2_eulerang 0.0 -120.0 0.0; 	  
    end relative;
    begin relative;
      mbdy1  hub1 1;
      mbdy2  strut1 1;
      mbdy2_eulerang 0.0 0.0 0.0; 
    end relative;
    begin relative;
      mbdy1  hub2 1;
      mbdy2  strut2 1;
      mbdy2_eulerang 0.0 0.0 0.0; 
    end relative;
    begin relative;
      mbdy1  hub3 1;
      mbdy2  strut3 1;
      mbdy2_eulerang 0.0 0.0 0.0; 
    end relative;
	begin relative;
	  mbdy1 strut1 last;         
	  mbdy2 blade1 2;
	  mbdy2_eulerang 0.0 0.0 0.0; 
	  mbdy2_eulerang 90.0 0.0 0.0;  
	end relative;   
    begin relative;
      mbdy1  strut2 last;         
      mbdy2  blade2 2;
	  mbdy2_eulerang 0.0 0.0 0.0;	  
	  mbdy2_eulerang 90.0 0.0 0.0;   
    end relative;
    begin relative;
      mbdy1  strut3 last;         
      mbdy2  blade3 2;
	  mbdy2_eulerang 0.0 0.0 0.0;	  
	  mbdy2_eulerang 90.0 0.0 0.0;   
    end relative;
	end orientation;
	begin constraint;
		begin fix0;  tower (fixed to ground in translation and rotation of node 1)
			mbdy tower;
		end fix0;
	 	begin bearing3;  shaft_rot (fixed-speed bearing)
	 		name  shaft_rot ;
	 		mbdy1 tower last;
	 		mbdy2 towertop 1;
	 		bearing_vector 2 0.0 0.0 1.0;        x=coo (0=global,1=mbdy1,2=mbdy2) vector in mbdy2 coordinates where the free rotation is present
	 		omegas 18.850 ; rad/s
	 	end bearing3; 
		begin fix1;  towertop to hub1
			mbdy1 towertop last;
			mbdy2 hub1 1;
		end fix1;
		begin fix1;  towertop to hub2
			mbdy1 towertop last;
			mbdy2 hub2 1;
		end fix1;
		begin fix1;  towertop to hub3
			mbdy1 towertop last;
			mbdy2 hub3 1;
		end fix1;
		begin fix1;  hub1 to strut1
		  mbdy1 hub1 last;
		  mbdy2 strut1 1;
		end fix1;
		begin fix1;  hub2 to strut2
		  mbdy1 hub2 last;
		  mbdy2 strut2 1;
		end fix1;
		begin fix1;  hub3 to strut3
		  mbdy1 hub3 last;
		  mbdy2 strut3 1;
		end fix1;
		begin fix1;  strut1 to blade1
		  mbdy1 strut1 last;
		  mbdy2 blade1 2;
		end fix1;
		begin fix1;  strut2 to blade2
		  mbdy1 strut2 last;
		  mbdy2 blade2 2;
		end fix1;
		begin fix1;  strut3 to blade3
		  mbdy1 strut3 last;
		  mbdy2 blade3 2;
		end fix1;
	end constraint;
end new_htc_structure;      
begin wind ;
  density                 1.225 ;
  wsp                     4  ;
  tint                    0.0 ; Turbulence intensity
  horizontal_input        1     ;            0=false, 1=true
  windfield_rotations     0.0  0.0  0.0 ;    yaw, tilt, rotation
  center_pos0             0.0 0.0 -5.0 ;
  shear_format            1  0.0  ;0=none,1=constant,2=log,3=power,4=linear
  turb_format             0  ;  0=none, 1=mann,2=flex
  tower_shadow_method     0  ;  0=none, 1,3=potential flow, 2,4=jet
end wind;
begin aero ;
  nblades  3;
  hub_vec towertop 3 ;          vector from hub (normal to rotor plane) directed towards tower top(+3)
  link 1 mbdy_c2_def blade1;
  link 2 mbdy_c2_def blade2;
  link 3 mbdy_c2_def blade3;
  ae_filename        ./data/H-rotor_SOL_0p08.ae ;  
  pc_filename        ./data/H-rotor.pc ;  
  induction_method   3 ;   0=none, 3=vawt
  aerocalc_method    1 ;     0=ingen aerodynamic, 1=med aerodynamic
  aerosections       30;
  ae_sets            1 1 1;
  tiploss_method     0 ;     0=none, 1=normal
  dynstall_method    0 ;     0=none, 1=stig ï¿½ye method,2=mhh method
end aero ;
begin output;
  filename ./res/H-rotor_3blades ;
  data_format gtsdf;
  time 100 150;
  buffer 500 ;  how many time steps to save in memory before writing to file
  deltat 0.005;  time step to write to output file
  ; general outputs
  general time;
  constraint bearing3 shaft_rot 3  ;  # shaft rotational speed
  wind free_wind 1 0.0 0.0 -5.0;  # wind speed at 5 m
  ; aerodynamic parameters
  aero omega;  "aerodynamic" rotor speed (average of blades)
  aero thrust;  integration of aero thrust
  aero torque;  aero torque, integration over all bladed sections (see note in manual)
  aero power;  product of aerodynamic rotor speed and aerodynamic torque
  aero int_force 3 1 2 0 # Blade 1 aerodynamic Fy;
  aero int_force 3 2 2 0 # Blade 2 aerodynamic Fy;
  aero int_force 3 3 2 0 # Blade 3 aerodynamic Fy;
  aero vrel 1 0.9144 ;  relative velocity at blade1 midpoint [m/s]
  aero alfa 1 0.9144 ;  AoA at blade1 midpoint [deg]
  aero lift 1 0.9144 ;  lift force at blade1 midpoint [kN/m]
  aero drag 1 0.9144 ;  drag force at blade1 midpoint [kN/m]
  ; forces and moments
  mbdy forcevec towertop 1 2 global   # Force at hub-height;
  mbdy momentvec towertop 1 2 global   # Moment at hub-height;  includes shaft torsion
end output;
exit;
